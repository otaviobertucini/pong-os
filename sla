all: driver

driver: ppos_disk.o queue.o ppos-core-task.o ppos-core-scheduler.o ppos-core-task-aux.o
	gcc -o driver ppos-core-task-aux.o ppos-core-scheduler.o ppos_disk.o queue.o ppos-core-task.o disk.o  -lrt

ppos_disk.o: disk.o
	gcc -c -Wall ppos_disk.c -lrt

disk.o: disk.c
	gcc -c -Wall disk.c -lrt